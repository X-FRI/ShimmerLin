{
  "name": "shimmer-lin",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build -p tsconfig.build.json",
    "dev": "NODE_ENV=development DEPLOY_ENV=dev nest start --watch -p tsconfig.build.json",
    "dev:prod": "NODE_ENV=production DEPLOY_ENV=prod nest start --watch -p tsconfig.build.json",
    "dev:staging": "NODE_ENV=development DEPLOY_ENV=staging nest start --watch -p tsconfig.build.json",
    "start:debug": "nest start --debug --watch -p tsconfig.build.json",
    "start:staging": "NODE_ENV=production DEPLOY_ENV=staging node dist/main",
    "start:prod": "NODE_ENV=production DEPLOY_ENV=prod node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma-gen": "npx prisma generate --schema prisma/schema/shimmer-lin.prisma"
  },
  "dependencies": {
    "@keyv/memcache": "^2.0.1",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.11",
    "@nestjs/cqrs": "^11.0.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^11.0.11",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.0.6",
    "@prisma/client": "^5.20.0",
    "axios": "^1.8.3",
    "bcrypt": "^5.1.1",
    "cache-manager": "^6.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "keyv": "^5.3.1",
    "nest-winston": "^1.10.2",
    "nestjs-cls": "^5.4.2",
    "openai": "^4.100.0",
    "prisma": "^5.20.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "save": "^2.9.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-console-format": "^1.0.8",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/testing": "^11.0.11",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/node-fetch": "^2.6.11",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "chalk": "^4.1.2",
    "jest": "^29.5.0",
    "json2csv": "^6.0.0-alpha.2",
    "prisma-dbml-generator": "^0.12.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.4"
  },
  "jest": {
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": ["**/*.(t|j)s"],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.build.json prisma/seeds/shimmer-lin-dev-seed.ts"
  }
}
